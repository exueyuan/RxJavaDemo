<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.duyi.rxjavademo">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.RxJavaBasicActivity"/>
        <activity android:name=".activity.CreateActivity"/>
        <activity android:name=".activity.TransformActivity"/>
        <activity android:name=".activity.CombinationActivity"/>
        <activity android:name=".activity.FunctionActivity"/>
        <activity android:name=".activity.FilterActivity"/>
        <activity android:name=".activity.ConditionActivity"/>
    </application>

</manifest>