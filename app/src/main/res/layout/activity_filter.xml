<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <Button android:id="@+id/bt_filter"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="5.1-filter"/>

        <Button android:id="@+id/bt_ofType"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="5.2-ofType"/>

        <Button android:id="@+id/bt_skip"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="5.3-skip"/>

        <Button android:id="@+id/bt_distinct"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="5.4-distinct"/>
        <Button android:id="@+id/bt_distinctUtilChanged"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="5.5-distinctUtilChanged"/>
        <Button android:id="@+id/bt_take"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="5.6-take"/>
        <Button android:id="@+id/bt_debounce"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="5.7-debounce"/>
        <Button android:id="@+id/bt_firstElement_lastElement"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="5.8-firstElement()_lastElement()"/>
        <Button android:id="@+id/bt_elementAt_elementAtOrError"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="5.9-elementAt()_elementAtOrError()"/>
    </LinearLayout>
</ScrollView>