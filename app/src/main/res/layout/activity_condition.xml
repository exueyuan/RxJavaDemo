<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <Button android:id="@+id/bt_all"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.1-all"/>
        <Button android:id="@+id/bt_takeWhile"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.2-takeWhile"/>
        <Button android:id="@+id/bt_skipWhile"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.3-skipWhile"/>
        <Button android:id="@+id/bt_takeUntil"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.4-takeUntil"/>
        <Button android:id="@+id/bt_skipUntil"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.5-skipUntil"/>
        <Button android:id="@+id/bt_sequenceEqual"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.6-sequenceEqual"/>
        <Button android:id="@+id/bt_contains"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.7-contains"/>
        <Button android:id="@+id/bt_isEmpty"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.8-isEmpty"/>
        <Button android:id="@+id/bt_amb"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.9-amb"/>
        <Button android:id="@+id/bt_defaultIfEmpty"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:text="6.10-defaultIfEmpty"/>
    </LinearLayout>
</ScrollView>