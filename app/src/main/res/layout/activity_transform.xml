<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <Button android:id="@+id/bt_map"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="2.1-map"/>
    <Button android:id="@+id/bt_flatMap"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="2.2-flatMap"/>
    <Button android:id="@+id/bt_concatMap"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="2.3-concatMap"/>
    <Button android:id="@+id/bt_buffer"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="2.4-buffer"/>
    <Button android:id="@+id/bt_groupBy"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="2.5-groupBy"/>
    <Button android:id="@+id/bt_scan"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="2.6-scan"/>
    <Button android:id="@+id/bt_window"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="2.7-window"/>
</LinearLayout>